        "expo-font",
        {
          "fonts": [
            "./assets/Poppins-Bold.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e1d77297-c847-40ba-a93b-3dd2c9a64d4f"
